#include <iostream>
#include <queue>
using namespace std;
void firstNonRepeating(string str) {
    int freq[256]={0};   
    queue<char> q;

    for (char c : str) {
        freq[c]++;       
        q.push(c);      

        
        while (!q.empty() && freq[q.front()] > 1) {
            q.pop();
        }

       
        if (q.empty()) {
           cout<<"-1";}
        else {
         cout<<q.front()<< " ";
        }
    }
   
}

int main() {
    string s="aabc";
    firstNonRepeating(s);
}
