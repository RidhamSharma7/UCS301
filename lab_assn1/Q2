#include <iostream>
using namespace std;
int arr[80]={1,2,2,3,4,4,5};
int n=7;
void removeDuplicates() {
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j<n;j++ ) {
            if (arr[i] == arr[j]) {
                for (int k =j; k<n-1; k++) {
                   arr[k] = arr[k+1];}
                n--;
            } else j++;
        }
    }
}
int main (){
cout<<"Original Array: ";
    for (int i = 0; i < n; i++)  {
 cout<<arr[i]<<" ";}
    cout << endl;

    removeDuplicates();

    cout << "after removing=";
    for (int i = 0; i < n; i++) {
 cout<<arr[i]<<" ";}
    cout << endl;
 
}
