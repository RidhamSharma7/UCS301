//Breadth First Search
#include <iostream>
#include <vector>
#include <queue>
using namespace std;

void BFS(int start, vector<vector<int>>& adj, int n) {
    vector<bool> visited(n, false);
    queue<int> q;

    visited[start] = true;
    q.push(start);

    while(!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";

        for(int nbr : adj[node]) {
            if(!visited[nbr]) {
                visited[nbr] = true;
                q.push(nbr);
            }
        }
    }
}

int main() {
    int n = 5;
    vector<vector<int>> adj = {
        {1, 2}, {0, 3}, {0, 3}, {1, 2, 4}, {3}
    };

    BFS(0, adj, n);
}
