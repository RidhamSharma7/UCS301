#include <iostream>
#include <vector>
using namespace std;

void DFS(int node, vector<vector<int>>& adj, vector<bool>& visited) {
    visited[node] = true;
    cout << node << " ";

    for(int nbr : adj[node]) {
        if(!visited[nbr])
            DFS(nbr, adj, visited);
    }
}

int main() {
    int n = 5;
    vector<vector<int>> adj = {
        {1, 2}, {0, 3}, {0, 3}, {1, 2, 4}, {3}
    };

    vector<bool> visited(n, false);
    DFS(0, adj, visited);
}
